<h1>Edit Photo</h1>

<form action="http://localhost:3000/update_photo/4">
  <div>
    <label for="photo_caption2">Caption:</label>
    <input type="text" name="the_caption" value="<%= @edit_photo.caption %>">
  </div>
  <div>
    <label for="photo_image_url">Image URL:</label>
    <input type="text" name="the_source" value="<%= @edit_photo.source %>">
  </div>
  <div>
    <button>Update Photo</button>
  </div>
</form>
